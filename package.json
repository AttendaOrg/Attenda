{
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.2.3",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint:ts": "tsc --noemit",
    "lint:eslint": "eslint src/**/*.{ts,tsx}",
    "lint": "npm run lint:ts && npm run lint:eslint",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "start-storybook-web": "node ./node_modules/@storybook/react/bin/index.js",
    "build-storybook-web": "node ./node_modules/@storybook/react/bin/build.js",
    "storybook:web": "yarn run start-storybook-web",
    "build-react": "webpack --mode production",
    "start-react": "webpack serve --config ./webpack.config.js --mode development",
    "prestorybook": "rnstl",
    "postinstall": "patch-package",
    "test:firebase-api": "cross-env NODE_ENV=testing FIREBASE_AUTH_EMULATOR_HOST='localhost:9099' FIRESTORE_EMULATOR_HOST='localhost:8080' jest --env=node --runInBand",
    "populate": "cross-env NODE_ENV=testing FIREBASE_AUTH_EMULATOR_HOST='localhost:9099' FIRESTORE_EMULATOR_HOST='localhost:8080' ts-node scripts/build.ts",
    "populate:prod": "cross-env PRESIST=none NODE_ENV=testing ts-node scripts/build.ts"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.{js,tsx}",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
