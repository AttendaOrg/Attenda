{
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.2.3",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint:ts": "tsc --noemit",
    "lint:eslint": "eslint src/**/*.{ts,tsx}",
    "lint": "npm run lint:ts && npm run lint:eslint",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "start-storybook-web": "node ./node_modules/@storybook/react/bin/index.js",
    "build-storybook-web": "node ./node_modules/@storybook/react/bin/build.js",
    "storybook:web": "yarn run start-storybook-web",
    "build-react": "webpack --mode production",
    "start-react": "webpack serve --config ./webpack.config.js --mode development",
    "prestorybook": "rnstl",
    "postinstall": "patch-package",
    "test:firebase-api": "cross-env NODE_ENV=testing FIREBASE_AUTH_EMULATOR_HOST='localhost:9099' FIRESTORE_EMULATOR_HOST='localhost:8080' jest --env=node --runInBand"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.{js,tsx}",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "@expo/webpack-config": "^0.12.58",
    "@react-native-community/datetimepicker": "3.0.0",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@types/react-native-calendars": "^1.505.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "expo": "~39.0.2",
    "expo-constants": "^10.0.0",
    "expo-status-bar": "~1.0.2",
    "firebase": "^8.2.1",
    "js-sha256": "^0.9.0",
    "patch-package": "^6.2.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
    "react-native-calendars": "^1.846.0",
    "react-native-check-box": "^2.1.7",
    "react-native-elements": "^3.0.0-alpha.1",
    "react-native-floating-action": "^1.21.0",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-paper": "^4.4.1",
    "react-native-popup-menu": "^0.15.10",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "12.1.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "~0.13.12",
    "rn-android-hotspot": "npm:hotspot-module-spec@^0.3.8",
    "rn-avatar": "^1.0.3",
    "toggle-switch-react-native": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.23",
    "@storybook/react": "^6.1.10",
    "@storybook/react-native": "^5.3.23",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.19",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-native-check-box": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "firebase-admin": "^9.4.2",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-native-storybook-loader": "^2.0.2",
    "typescript": "~3.9.5"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --cache --fix"
  }
}
